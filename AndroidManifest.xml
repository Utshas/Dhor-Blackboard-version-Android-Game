<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tazwar.dhor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tazwar.dhor.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.tazwar.dhor.Survival"
            android:label="@string/title_activity_survival"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.tazwar.dhor.TimeLimit"
            android:label="@string/title_activity_time_limit"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.tazwar.dhor.Achievements"
            android:label="@string/title_activity_achievements"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.tazwar.dhor.Splash_screen"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
